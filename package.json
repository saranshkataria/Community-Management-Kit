{
  "name": "community-management-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-client-preset": "1.0.8",
    "concurrently": "3.6.0",
    "esm": "3.0.61",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "graphql-yoga": "1.14.10",
    "react": "16.4.1",
    "react-apollo": "2.1.8",
    "react-dom": "16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start-backend": "node -r esm server/index.js",
    "start-frontend": "react-scripts start",
    "build-frontend": "react-scripts build",
    "test-frontend": "react-scripts test --env=jsdom",
    "eject-frontend": "react-scripts eject",
